          "settings": {
            "image": "shopify:\/\/shop_images\/About_Coffee-Food-Pairing-Sweet.webp",
            "title": "<strong>Sweetness<\/strong>",
            "text": "<p>Naturally sweet or caramelized notes in coffee can balance out savory or salty foods.<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_EMzqBd": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/About_Coffee-Food-Pairing-Bitterness.webp",
            "title": "<strong>Bitterness<\/strong>",
            "text": "<p>Dark roasts with their inherent bitterness can contrast with sweet desserts or rich, creamy foods.<\/p>",
            "link_label": "",
            "link": ""
          }
        }
      },
